<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_type_pdp" model="ir.sequence.type">
            <field name="name">Plan de production</field>
            <field name="code">pdp</field>
        </record>

        <record id="seq_pdp" model="ir.sequence">
            <field name="name">Plan de production</field>
            <field name="code">pdp</field>
            <field name="prefix">PDP/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_cbn" model="ir.sequence.type">
            <field name="name">Calcul des besoins</field>
            <field name="code">cbn</field>
        </record>

        <record id="seq_cbn" model="ir.sequence">
            <field name="name">Calcul des besoin net</field>
            <field name="code">cbn</field>
            <field name="prefix">CBN/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
          <record id="seq_type_dai" model="ir.sequence.type">
            <field name="name">Demande d'achat informatisée</field>
            <field name="code">dai</field>
        </record>

          <record id="seq_dai" model="ir.sequence">
            <field name="name">Demande d'achat informatisée</field>
            <field name="code">dai</field>
            <field name="prefix">DAI/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
